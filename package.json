{
  "name": "dapp-browser",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=local vue-cli-service serve ./src/main.js",
    "build": "rimraf ./dist && NODE_ENV=production vue-cli-service build ./src/main.js",
    "build:dev": "rimraf ./dist && NODE_ENV=development vue-cli-service build ./src/main.js",
    "lint": "vue-cli-service lint",
    "test:e2e": "vue-cli-service test:e2e",
    "test": "vue-cli-service test:unit",
    "format": "prettier --write 'src/*.{js,vue,json}' && eslint src/*.{js,vue} --fix"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "src/*.{js,vue,json}": [
      "prettier --write",
      "git add"
    ],
    "tests/*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@endpass/class": "^0.9.4",
    "@endpass/connect": "^0.16.1-beta",
    "@endpass/utils": "^1.2.1",
    "bip39": "^2.5.0",
    "ethereum-blockies-base64": "^1.0.2",
    "ethereumjs-wallet": "^0.6.3",
    "lodash": "^4.17.11",
    "reset.css": "^2.0.2",
    "vue": "^2.5.17",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "web3": "1.0.0-beta.37"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.4.1",
    "@vue/cli-plugin-e2e-cypress": "^3.4.1",
    "@vue/cli-plugin-eslint": "^3.4.1",
    "@vue/cli-plugin-unit-jest": "^3.4.1",
    "@vue/cli-service": "^3.2.0",
    "@vue/eslint-config-prettier": "^4.0.1",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-lodash": "^3.3.4",
    "commitizen": "^3.0.7",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.14.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-vue": "^5.2.2",
    "html-webpack-plugin": "^3.2.0",
    "lint-staged": "^8.1.4",
    "postcss": "^7.0.14",
    "postcss-nested": "^4.1.2",
    "prettier": "^1.16.4",
    "rimraf": "^2.6.3",
    "semantic-release": "^17.2.3",
    "svgo": "^1.2.0",
    "svgo-loader": "^2.2.0",
    "vue-cli-plugin-svg": "^0.1.2",
    "vue-cli-plugin-svg-sprite": "^0.3.1",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^3.0.4",
    "webpack-cli": "^3.2.3"
  }
}
